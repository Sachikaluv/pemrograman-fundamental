package tugas2;

import javax.swing.JOptionPane;
public class tugas2{
	public static void main(String[] asg){
		String username = JOptionPane.showInputDialog("Username");
		String password = JOptionPane.showInputDialog("Password");
		if ( username !=null && password != null
		&& (
			(username.equals("salsa") && password.equals("327")) ||
			(username.equals("sanji") && password.equals("723"))
			)
			)
			{
				JOptionPane.showMessageDialog(null, "Hallo, semoga hari mu indah.");
			} else {
				JOptionPane.showMessageDialog(null, " maaf password nya salah.");
			}
	}
}